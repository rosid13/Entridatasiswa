[debug] [2025-06-30T22:33:05.663Z] >>> [apiv2][body] PUT http://127.0.0.1:8080/emulator/v1/projects/demo-app:securityRules {"ignore_errors":true,"rules":{"files":[{"name":"security.rules","content":"rules_version = '2';\n\nservice cloud.firestore {\n  match /databases/{database}/documents {\n\n    function isAdmin() {\n      // Pastikan pengguna terotentikasi sebelum mencoba membaca peran\n      return request.auth != null && get(/databases/$(database)/documents/userRoles/$(request.auth.uid)).data.role == 'admin';\n    }\n\n    function isAuthenticated() {\n      return request.auth != null;\n    }\n\n    match /siswa/{studentId} {\n      allow read, create: if isAuthenticated();\n      allow update, delete: if isAdmin();\n    }\n\n    match /userRoles/{userId} {\n      allow read: if isAuthenticated();\n      allow create: if isAuthenticated() && request.resource.data.role == 'user';\n      // Hanya admin yang bisa mengubah peran seseorang\n      allow update: if isAdmin();\n    }\n    \n    match /correctionRequests/{requestId} {\n      // Pengguna terotentikasi dapat membuat permintaan\n      allow create: if isAuthenticated();\n      // Hanya admin yang dapat membaca dan memperbarui (untuk menyetujui/menolak)\n      allow read, update: if isAdmin();\n      // Hapus tidak diizinkan untuk menjaga jejak audit\n      allow delete: if false;\n    }\n  }\n}\n"}]}}
